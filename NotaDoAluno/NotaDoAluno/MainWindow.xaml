<Window x:Class="NotaDoAluno.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NotaDoAluno"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="85*"/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="dataGrid" Height="360" Margin="10,49,22,0" VerticalAlignment="Top" BorderBrush="#FFEC6D15" Foreground="#FF1A4C95" Background="#FF5A6131" Grid.ColumnSpan="2"/>
        <Button Content="Resultado" Margin="10,10,0,0" VerticalAlignment="Top" Height="58" Click="Button_Click" SnapsToDevicePixels="True" HorizontalAlignment="Left" Width="782" BorderBrush="#FFE80909" Foreground="#FFF71919" Background="#FF9FB88F" Grid.ColumnSpan="2">
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>

    </Grid>
</Window>
